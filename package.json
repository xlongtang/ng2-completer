{
  "name": "ng2-completer",
  "version": "1.7.1",
  "description": "ng2 autocomplete/typeahead component",
  "main": "bundles/ng2-completer.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "cross-env ./node_modules/.bin/del  .tmp bundles dist \"src/**/*.+(js|d.ts|js.map)\" \"demo/**/*.+(js|js.map)\" && mkdir -p dist",
    "prebuild": "npm run clean",
    "build": "npm run build:ngc && npm run bundle:umd && npm run packagejson && npm run copy",
    "build:ngc": "cross-env ./node_modules/.bin/ngc -p tsconfig.bundle.json",
    "postbuild:ngc": "cross-env ./node_modules/.bin/ncp .tmp/esm/src bundles",
    "copy": "cross-env node copy-bundles.js",
    "packagejson": "node ./create-package-json.js",
    "bundle:umd": "cross-env ./node_modules/.bin/rollup -c rollup.config.js && zopfli bundles/ng2-completer.umd.js",
    "build:demo:dev": "cross-env webpack --progress --color",
    "build:demo:prod": "cross-env NODE_ENV=production webpack --config config/webpack.prod.js --progress --color",
    "start": "cross-env webpack-dev-server --inline --progress --port 8080",
    "deploy:gh-pages": "npm run build:demo:prod && cross-env ./node_modules/.bin/gh-pages -d dist"
  },
  "keywords": [
    "angular2",
    "autcomplete",
    "typeahead",
    "word completion",
    "ng2"
  ],
  "author": "Ofer Herman <oferhe@gmail.com>",
  "contributors": [
    {
      "name": "Andrew Rumm",
      "email": "rayz@rayz.ru"
    }
  ],
  "bugs": {
    "url": "https://github.com/oferh/ng2-completer/issues"
  },
  "license": "MIT",
  "typings": "./bundles/src/index.d.ts",
  "repository": "oferh/ng2-completer",
  "dependencies": {
    "@angular/common": "^5.2.5",
    "@angular/compiler": "^5.2.5",
    "@angular/core": "^5.2.5",
    "@angular/forms": "^5.2.5",
    "@angular/http": "^5.2.5",
    "@angular/platform-browser": "^5.2.5",
    "@angular/platform-browser-dynamic": "^5.2.5",
    "rxjs": "^5.5.6",
    "zone.js": "^0.8.20"
  },
  "devDependencies": {
    "@angular/animations": "^5.2.5",
    "@angular/cdk": "^5.2.1",
    "@angular/compiler-cli": "^5.2.5",
    "@angular/material": "^5.2.1",
    "@angular/platform-server": "^5.2.5",
    "@angular/router": "^5.2.5",
    "@types/node": "~9.4.6",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^3.4.1",
    "copy-webpack-plugin": "^4.4.1",
    "core-js": "^2.5.3",
    "cpr": "^3.0.1",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.9",
    "del-cli": "^1.1.0",
    "ejs": "^2.5.7",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.7",
    "gh-pages": "^1.1.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^2.30.1",
    "ncp": "^2.0.0",
    "ngx-bootstrap": "^2.0.2",
    "node-zopfli": "^2.0.2",
    "object.assign": "^4.1.0",
    "raw-loader": "^0.5.1",
    "reflect-metadata": "^0.1.12",
    "rollup": "^0.56.2",
    "rollup-plugin-typescript2": "^0.11.1",
    "rollup-plugin-uglify": "^3.0.0",
    "run-sequence": "^2.2.1",
    "style-loader": "^0.20.2",
    "tslint": "^5.9.1",
    "typescript": "2.6.2",
    "uglify-js": "^3.3.11",
    "webpack": "~3.11.0",
    "webpack-dev-server": "~2.11.1",
    "webpack-merge": "^4.1.1"
  }
}
